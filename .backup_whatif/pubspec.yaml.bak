name: whatifapp
description: Un'app Flutter per esplorare scenari WhatIf
publish_to: "none"
version: 1.0.0+1

environment:
  sdk: ">=3.0.0 <4.0.0"

dependencies:
  flutter_svg: ^2.0.9
  shared_preferences: ^2.3.2
  flutter:
    sdk: flutter

  cupertino_icons: ^1.0.8
  provider: ^6.1.5+1
  go_router: ^6.2.2
  http: ^1.2.2
  google_fonts: ^6.2.1

dev_dependencies:
  flutter_test:
    sdk: flutter
  flutter_lints: ^2.0.0

flutter:
  uses-material-design: true

  assets:
    - assets/whataf_bar.svg
    - assets/whatif_door.svg
    - assets/
